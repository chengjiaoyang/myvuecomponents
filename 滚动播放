                            <script>
                                var speed=66;
                                var ktgg=document.getElementById("ktgg");
                                var ktgg2=document.getElementById("ktgg2");
                                var ktgg1=document.getElementById("ktgg1");
                                    ktgg2.innerHTML=ktgg1.innerHTML
                                function Marquee(){
                                    if(ktgg2.offsetTop-ktgg.scrollTop<=0)
                                        ktgg.scrollTop-=ktgg1.offsetHeight
                                    else{
                                        ktgg.scrollTop++
                                    }
                                }
                                var MyMar=setInterval(Marquee,speed)
                                ktgg.onmouseover=function() {clearInterval(MyMar)}
                                ktgg.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
                            </script>
